<Window x:Class="WpfApplication2.GridWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GridWindow" Height="356" Width="696.4" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0" x:Name="beyandGrid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="30px"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="0.5in"/>
                <RowDefinition Height="1cm"/>
                <RowDefinition Height="30pt"/>
            </Grid.RowDefinitions>
        </Grid>
        <Grid x:Name="behindGrid" Grid.Column="1">
        </Grid>
    </Grid>
</Window>
